  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for collection to do

  target 'collection to doTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'collection to doUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
